<o:EventHandlerTargetItem Id="o31">
<a:Name>Initialize</a:Name>
<a:Comment>DFD</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HandlerScript>Function %Initialize%(obj)
   &#39; skript kontroluje splnění
   &#39; formálních podmínek konstrukce DFD
   
%Initialize% = True
Dim ClassA, ClassB
ClassA = obj.ClassA
ClassB = obj.ClassB

obj.RoleAMultiplicity = &quot;&quot;                                     &#39; Odstraní kardinality u Data Flow
obj.RoleBMultiplicity = &quot;&quot;                                     &#39; již při vytvoření

If obj.ClassA.Stereotype = &quot;Terminator&quot; then                   &#39; Pokud je vytvářen Data Flow jako vstupní, tj. od Terminátoru,
  obj.Stereotype = &quot;Input Data Flow&quot;                           &#39; 
End if

If ClassA = ClassB then                                        &#39; kontroluje přítomnost rekurzivního Datového toku
  Msgbox &quot;Recursive Data Flow from &quot; + ClassA                  &#39; a maže ho
  obj.Delete
else
  If ((obj.ClassA.StereoType = &quot;Data Store&quot;) or (obj.ClassA.StereoType = &quot;Complex Data Store&quot;)) and (obj.ClassB.StereoType = &quot;Terminator&quot;) then
    MsgBox &quot;Terminator &quot; + ClassB + &quot; may not read data directly from Data Store &quot; + ClassA
    obj.Delete
  else
    If (obj.ClassA.StereoType = &quot;Terminator&quot;) and ((obj.ClassB.StereoType = &quot;Data Store&quot;) or (obj.ClassB.StereoType = &quot;Complex Data Store&quot;)) then
      MsgBox &quot;Terminator &quot; + ClassA + &quot; may not write data directly to Data Store &quot; + ClassB
      obj.Delete 
    else  
      Select Case obj.ClassA.Stereotype                        &#39; kontroluje, jestli nejsou spojeny Datovým tokem
        Case &quot;Data Store&quot;, &quot;Complex Data Store&quot;, &quot;Terminator&quot;  &#39; 2 objekty stereotypu Data Store, Complex Data Store nebo Terminator
        If (obj.ClassA.StereoType = obj.ClassB.StereoType) or ((obj.ClassA.StereoType = &quot;Data Store&quot;) and (obj.ClassB.StereoType = &quot;Complex Data Store&quot;)) or ((obj.ClassA.StereoType = &quot;Complex Data Store&quot;) and (obj.ClassB.StereoType = &quot;Data Store&quot;)) then
          MsgBox &quot;Terminators and Data Stores may be connected via Function only&quot;
          obj.Delete
        End if
      End Select
    End if
  End if
End if

End Function</a:HandlerScript>
</o:EventHandlerTargetItem>
